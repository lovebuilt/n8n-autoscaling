{
	"task-runners": [
		{
			"runner-type": "javascript",
			"workdir": "/home/runner",
			"command": "/usr/local/bin/node",
			"args": [
				"/opt/runners/task-runner-javascript/dist/start.js"
			],
			"health-check-server-port": "5681",
			"allowed-env": [
				"PATH",
				"GENERIC_TIMEZONE",
				"NODE_OPTIONS",
				"N8N_RUNNERS_AUTO_SHUTDOWN_TIMEOUT",
				"N8N_RUNNERS_TASK_TIMEOUT",
				"N8N_RUNNERS_MAX_CONCURRENCY",
				"N8N_SENTRY_DSN",
				"N8N_VERSION",
				"ENVIRONMENT",
				"DEPLOYMENT_NAME",
				"HOME",
				"PUPPETEER_EXECUTABLE_PATH",
				"PUPPETEER_SKIP_CHROMIUM_DOWNLOAD",
				"PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD",
				"PLAYWRIGHT_CHROMIUM_EXECUTABLE_PATH"
			],
			"env-overrides": {
				"NODE_ENV": "test",
				"NODE_FUNCTION_ALLOW_BUILTIN": "crypto,path,fs,url",
				"NODE_FUNCTION_ALLOW_EXTERNAL": "moment,ajv,ajv-formats,puppeteer-core,puppeteer-extra,puppeteer-extra-plugin-stealth,playwright-core,playwright-extra",
				"N8N_RUNNERS_HEALTH_CHECK_SERVER_HOST": "0.0.0.0",
				"PUPPETEER_EXECUTABLE_PATH": "/usr/bin/chromium-browser",
				"PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
				"PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1",
				"PLAYWRIGHT_CHROMIUM_EXECUTABLE_PATH": "/usr/bin/chromium-browser"
			}
		},
		{
			"runner-type": "python",
			"workdir": "/home/runner",
			"command": "/opt/runners/task-runner-python/.venv/bin/python",
			"args": ["-m", "src.main"],
			"health-check-server-port": "5682",
			"allowed-env": [
				"PATH",
				"N8N_RUNNERS_LAUNCHER_LOG_LEVEL",
				"N8N_RUNNERS_AUTO_SHUTDOWN_TIMEOUT",
				"N8N_RUNNERS_TASK_TIMEOUT",
				"N8N_RUNNERS_MAX_CONCURRENCY",
				"N8N_SENTRY_DSN",
				"N8N_VERSION",
				"ENVIRONMENT",
				"DEPLOYMENT_NAME"
			],
			"env-overrides": {
				"PYTHONPATH": "/opt/runners/task-runner-python",
				"N8N_RUNNERS_STDLIB_ALLOW": "datetime,json,math,os,re,io,base64,hashlib,urllib,subprocess",
				"N8N_RUNNERS_EXTERNAL_ALLOW": "requests,pillow,PIL,pandas,numpy"
			}
		}
	]
}
